<article class="challenge-lobby">
  {% include 'wyciwyg:sdc_back' %}
  <div class="content">
    <h1>{{ label }}</h1>
    <div class="display-time-up">
      Time is up!
    </div>
    <div class="grid"> 
      {# <div class="player-list">
        <p>Players</p>
      </div> #}
      <div class="timer" data-state="paused">
        <p>Timer</p>
        <span data-timer="{{ time }}">{{ time }}</span>
        <button data-timer-button="start">Start</button>
        <button data-timer-button="pause">Pause</button>
        <button data-timer-button="reset" class="reset-button">Reset</button>
      </div>
      <div class="gamepin">
        <span>{{ game_pin }}</span>
        <p>Game PIN</p>
      </div>
    </div>
  </div>
</article>